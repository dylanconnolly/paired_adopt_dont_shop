Edit Review

<%= form_tag "/shelters/#{@shelter.id}/#{@review.id}", method: :patch do %>
  <%= label_tag :title %>
  <%= text_field_tag :title, "#{@review.title}" %>
  <%= label_tag :rating %>
  <%= text_field_tag :rating, "#{@review.rating}" %>
  <%= label_tag :content %>
  <%= text_field_tag :content, "#{@review.content}" %>
  <%= label_tag :img %>
  <%= text_field_tag :img, "#{@review.img}" %>

  <%= submit_tag "Update Review" %>
<% end %>
